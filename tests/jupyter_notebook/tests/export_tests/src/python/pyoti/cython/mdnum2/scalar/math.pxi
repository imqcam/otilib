cpdef mdnum2_t mdnum2_t_cos(mdnum2_t val):
  """
  PURPOSE:  Mathematical function of cosine for OTI numbers
  """
  #***************************************************************************************************

  
  
  cdef mdnum2_t res = mdnum2_cos(&val.num)

  return mdnum2_t.create(&res)
